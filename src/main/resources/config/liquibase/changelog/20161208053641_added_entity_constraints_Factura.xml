<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity Factura.
    -->
    <changeSet id="20161208053641-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="cliente_id"
                                 baseTableName="factura"
                                 constraintName="fk_factura_cliente_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cliente"/>

        <addForeignKeyConstraint baseColumnNames="servicio_id"
                                 baseTableName="factura"
                                 constraintName="fk_factura_servicio_id"
                                 referencedColumnNames="id"
                                 referencedTableName="servicio"/>

    </changeSet>
</databaseChangeLog>
