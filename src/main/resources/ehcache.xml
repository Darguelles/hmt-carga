<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd"
         name="CM1"
         updateCheck="false"
         maxBytesLocalHeap="16M">

    <!--
      This is a default configuration, it is re-configured by the CacheConfiguration Spring Bean, using the
      properties from the resources/config/*.yml files.
    -->

    <diskStore path="java.io.tmpdir"/>

    <defaultCache
            eternal="false"
            overflowToDisk="false"
            />

    <cache name="com.hmt.carga.domain.User"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.hmt.carga.domain.Authority"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.hmt.carga.domain.User.authorities"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.hmt.carga.domain.PersistentToken"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.hmt.carga.domain.User.persistentTokens"
           timeToLiveSeconds="3600">
    </cache>
    
    <cache name="com.hmt.carga.domain.TipoUnidad"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.hmt.carga.domain.Transporte"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.hmt.carga.domain.Servicio"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.hmt.carga.domain.FormaPago"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.hmt.carga.domain.CondicionPago"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.hmt.carga.domain.Cliente"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.hmt.carga.domain.Proveedor"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.hmt.carga.domain.Cotizacion"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.hmt.carga.domain.GuiaRemision"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.hmt.carga.domain.OrdenVenta"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.hmt.carga.domain.Factura"
        timeToLiveSeconds="3600">
    </cache>

    <!-- jhipster-needle-ehcache-add-entry -->
</ehcache>
